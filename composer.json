{
    "name": "hipcall\/telnyx-php",
    "description": "Telnyx PHP Library from Hipcall",
    "keywords": [
        "telnyx",
        "api",
        "hipcall"
    ],
    "homepage": "https:\/\/wwww.hipcall.com\/",
    "license": "MIT",
    "authors": [
        {
            "name": "Telnyx and contributors",
            "homepage": "https:\/\/github.com\/telnyx\/telnyx-php\/contributors"
        },
        {
            "name": "Hipcall Developer Team",
            "homepage": "https:\/\/github.com\/hipcall\/telnyx-php\/graphs\/contributors"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "paragonie/sodium_compat": "^1.13"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "squizlabs/php_codesniffer": "^3.3",
      "symfony/process": "~3.4",
      "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
      "psr-4": { "Telnyx\\" : "lib/" }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover build/logs/clover.xml",
        "cs-check": "phpcs"
    }
}
